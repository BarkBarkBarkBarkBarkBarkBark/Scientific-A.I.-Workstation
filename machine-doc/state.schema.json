{
  "store": "zustand",
  "file": "src/store/useSawStore.ts",
  "state": {
    "graph": { "nodes": "PluginNode[]", "edges": "Edge[]" },
    "selection": { "selectedNodeId": "string|null" },
    "modes": {
      "layoutMode": "'pipeline'|'graph'",
      "editableMode": "bool"
    },
    "modals": {
      "editor": "{open:bool,nodeId:string|null}",
      "fullscreen": "{open:bool,nodeId:string|null}",
      "consoleFullscreen": "bool"
    },
    "console": { "bottomTab": "'logs'|'errors'|'ai'|'chat'|'dev'" },
    "panels": {
      "layout": "{leftWidth:number,leftWidthOpen:number,leftCollapsed:bool,rightWidth:number,bottomHeight:number}"
    },
    "streams": { "logs": "string[]", "errors": "string[]", "aiMessages": "string[]" },
    "ai": { "aiStatus": "AiStatus|null", "aiBusy": "bool" },
    "chat": { "chatBusy": "bool", "chat.messages": "ChatMessage[]" }
  },
  "actions": [
    "addNodeFromPlugin(pluginId,pos)",
    "addNodeFromPluginAtIndex(pluginId,index)",
    "moveNodeInPipeline(fromIndex,toIndex)",
    "deleteNode(nodeId)",
    "deleteSelectedNode()",
    "reflowPipeline()",
    "updateNodeParam(nodeId,paramId,value)",
    "openEditor(nodeId)/closeEditor()",
    "openFullscreen(nodeId)/closeFullscreen()",
    "openConsoleFullscreen()/closeConsoleFullscreen()",
    "toggleLeftSidebar()",
    "setLayout(patch)",
    "submitGoal(goal): Promise",
    "sendChat(text): Promise",
    "loadMp3ToNode(nodeId,file): Promise",
    "recomputeLowpass(nodeId): Promise"
  ]
}


